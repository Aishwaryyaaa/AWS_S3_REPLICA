pipeline {
    agent {
        label 'maven'
          }
    stages {
        stage ('gitpull'){
            steps {
                git 'https://github.com/Aishwaryyaaa/student-ui.git'
            }
        }
        stage ('install maven'){
            steps {
                sh 'apt update'
                sh 'apt install maven -y'
            }
        }
    }
    }
